[layer_names]

2d_physics/layer_1="terrain"
2d_physics/layer_2="player"
2d_physics/layer_3="hazard"
2d_physics/layer_4="bumpable object"
2d_physics/layer_5="trickable object"
2d_physics/layer_9="boost tile"
2d_physics/layer_10="duck blocker"

[physics]

common/physics_jitter_fix=0.0
2d/solver/solver_iterations=32
common/physics_interpolation=true

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/canvas_textures/default_texture_repeat=1
2d/snap/snap_2d_transforms_to_pixel=true
textures/size/viewport_height=120
textures/size/viewport_width=360
